// HTML - Выучить вот эти теги 

<!-- Вот так ставиться комент в html  -->
<div></div> <!-- Блок -->
<p></p> <!-- Абзац -->
<a href='Ссылка'>Анкор</a> <!-- Ссылка -->

<!-- Теги с которых складывается таблица  -->
<table>         <!-- Открытие таблицы  -->
  <tr>          <!-- Открытие рядка -->
    <td> Тест 1 </td>   <!-- Столбец 1 -->
    <td> Тест 2  </td>   <!-- Столбец 2 -->
  </tr>         <!-- Закрытие рядка -->
</table>        <!-- Закрытие таблицы  -->


//
